<spirit:abstractionDefinition xmlns:spirit="http://www.spiritconsortium.org/XMLSchema/SPIRIT/1685-2009" xmlns:arm="http://www.arm.com/SPIRIT/1685-2009" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.spiritconsortium.org/XMLSchema/SPIRIT/1685-2009 http://www.spiritconsortium.org/XMLSchema/SPIRIT/1685-2009/index.xsd">
    <spirit:vendor>amba.com</spirit:vendor>
    <spirit:library>AMBA5</spirit:library>
    <spirit:name>LTI_rtl</spirit:name>
    <spirit:version>r0p1_0</spirit:version>
    <spirit:busType spirit:vendor="amba.com" spirit:library="AMBA5" spirit:name="LTI" spirit:version="r0p1_0"/>
    <spirit:ports>
        <spirit:port>
            <spirit:logicalName>CLK</spirit:logicalName>
            <spirit:description>Global clock signal. Other signals that are not defined as asynchronous are sampled on the rising edge of CLK.</spirit:description>
            <spirit:wire>
                <spirit:qualifier>
                    <spirit:isClock>true</spirit:isClock>
                </spirit:qualifier>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:requiresDriver spirit:driverType="clock">true</spirit:requiresDriver>
            </spirit:wire>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>CLKEN</spirit:logicalName>
            <spirit:description>Clock enable signal. Used as a qualifier for the CLK signal. This signal indicates which rising edges of CLK should be acted upon: 1 = valid rising edge of CLK, 0 = any rising edge of CLK should be ignored and no bus-state altered.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>1</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:isClockEnable>true</arm:isClockEnable>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>RESETn</spirit:logicalName>
            <spirit:description>Global reset signal. This signal is active LOW.</spirit:description>
            <spirit:wire>
                <spirit:qualifier>
                    <spirit:isReset>true</spirit:isReset>
                </spirit:qualifier>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:requiresDriver spirit:driverType="singleShot">true</spirit:requiresDriver>
            </spirit:wire>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAVALID</spirit:logicalName>
            <spirit:description>Channel valid.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAVC</spirit:logicalName>
            <spirit:description>Virtual Channel. Width is ceil(clog2(LTI_VC_COUNT)).</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LACREDIT</spirit:logicalName>
            <spirit:description>Channel credit grant. Width is LTI_VC_COUNT.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAID</spirit:logicalName>
            <spirit:description>Translation request ID. Width is LTI_ID_WIDTH.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAOGV</spirit:logicalName>
            <spirit:description>Order Group valid.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAOG</spirit:logicalName>
            <spirit:description>Order group. Width is LTI_OG_WIDTH.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAFLOW</spirit:logicalName>
            <spirit:description>Indicates the translation flow required. This signal is present only when LTI_MMU is True.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>2</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>2</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:defaultNotDefined>Unknown</arm:defaultNotDefined>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAMMUV</spirit:logicalName>
            <spirit:description>Indicates if SMMU translation is required.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LASECSID</spirit:logicalName>
            <spirit:description>StreamID security level. Width is 1 when LTI_GPC==False, otherwise is 2. This signal is present only when LTI_MMU is True.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LASID</spirit:logicalName>
            <spirit:description>StreamID. Width is LTI_SID_WIDTH. This signal is present only when LTI_MMU is True.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:defaultNotDefined>Unknown</arm:defaultNotDefined>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LASSIDV</spirit:logicalName>
            <spirit:description>SubstreamID valid. Not present if LTI_SSID_WIDTH==0. This signal is present only when LTI_MMU is True.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:defaultNotDefined>Unknown</arm:defaultNotDefined>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LASSID</spirit:logicalName>
            <spirit:description>SubstreamID. Width is LTI_SSID_WIDTH. This signal is present only when LTI_MMU is True.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:defaultNotDefined>Unknown</arm:defaultNotDefined>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAPROT</spirit:logicalName>
            <spirit:description>Protection information. This uses the same encoding as AXI AxPROT. LAPROT[0] and LAPROT[2] is present only when LTI_MMU is True.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:defaultNotDefined>Unknown</arm:defaultNotDefined>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LANSE</spirit:logicalName>
            <spirit:description>RME information. Used in conjunction with LAPROT[1] to define Physical Address Space of the transaction, equivalent to its use in AXI.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAADDR</spirit:logicalName>
            <spirit:description>Address. Width is 64-bit when LTI_MMU is True, otherwise is LTI_LRADDR_WIDTH.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LATRANS</spirit:logicalName>
            <spirit:description>Type of the transaction which the LTI request is translating.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>4</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>4</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAATTR</spirit:logicalName>
            <spirit:description>Attributes for the untranslated transaction.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>4</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>4</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAHWATTR</spirit:logicalName>
            <spirit:description>Implementation defined hardware attributes, present when LTI_LAHWATTR_PRESENT is True.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>4</spirit:width>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:width>4</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LALOOP</spirit:logicalName>
            <spirit:description>Implementation defined loopback signaling. Width is LTI_LOOP_WIDTH.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LATLBLOC</spirit:logicalName>
            <spirit:description>Location to access in the TLB. This meaning of this signal is implementation defined. Width is LTI_TLBLOC_WIDTH.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAUSER</spirit:logicalName>
            <spirit:description>Implementation defined additional signaling. Width is LTI_LAUSER_WIDTH.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>in</spirit:direction>
                </spirit:onSlave>
                <spirit:defaultValue>0</spirit:defaultValue>
            </spirit:wire>
            <spirit:vendorExtensions>
                <arm:channelRef>Request</arm:channelRef>
            </spirit:vendorExtensions>
        </spirit:port>
        <spirit:port>
            <spirit:logicalName>LAMECID</spirit:logicalName>
            <spirit:description>Memory Encryption Context Identifier. Width is LTI_MECID_WIDTH.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>optional</spirit:presence>
                    <spirit:direction>out</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>optional</spirit:presence>
