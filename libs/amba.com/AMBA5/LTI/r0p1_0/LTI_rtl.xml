        <spirit:port>
            <spirit:logicalName>LRCTAG</spirit:logicalName>
            <spirit:description>Translation completion tag.</spirit:description>
            <spirit:wire>
                <spirit:onMaster>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>1</spirit:width>
                    <spirit:direction>in</spirit:direction>
                </spirit:onMaster>
                <spirit:onSlave>
                    <spirit:presence>required</spirit:presence>
                    <spirit:width>1</spirit:width>
